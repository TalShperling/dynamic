{
  "name": "graphql-apollo-server-express-typescript-seed",
  "version": "1.0.0",
  "description": "GraphQL Apollo-Server-Express Typescript seed",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon src/server.ts",
    "poststart": "npm run generate",
    "dev-with-watch": "concurrently \"nodemon src/server.ts\" \"npm run generate\"",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "npx eslint --fix ./src/**",
    "prepare": "npm run generate",
    "start": "npm run build && node dist/server.js",
    "start:prod": "node dist/server.js",
    "debug": "tsc && node --inspect-brk dist/server"
  },
  "keywords": [
    "apollo-server-express",
    "graphql",
    "typescript",
    "seed"
  ],
  "author": "Tal Shperling & Avi Levi & Itamar Goldshlager",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "2.1.1",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.1.2",
    "@graphql-codegen/typescript-resolvers": "2.1.2",
    "@types/axios": "^0.14.0",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.166",
    "@types/node": "14.17.12",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "concurrently": "6.2.1",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "jest": "27.0.6",
    "jsdoc": "3.6.6",
    "nodemon": "2.0.12",
    "prettier": "2.2.0",
    "ts-node": "9.0.0",
    "typescript": "4.4.2",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "apollo-link-http": "^1.5.17",
    "apollo-server-express": "^3.0.0",
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.6",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "lodash": "^4.17.20",
    "merge-graphql-schemas": "^1.7.8",
    "winston": "^3.3.3",
    "winston3-logstash-udp": "0.0.7"
  }
}
