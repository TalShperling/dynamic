{
  "name": "bluetorch-dynamic",
  "version": "1.0.0",
  "description": "GraphQL Apollo-Server-Express Typescript seed",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "debug": "tsc && node --inspect-brk dist/server",
    "format": "npm run prettier && npm run lint",
    "generate": "wait-on tcp:8000 && graphql-codegen --config codegen.yml",
    "lint": "npx eslint --fix ./src/**",
    "prettier": "npx prettier --write ./src/",
    "start": "concurrently -k -n \"Server,GQL-Codegen\" -c \"blue,magenta\" \"nodemon src/server.ts\" \"npm run generate\"",
    "start:prod": "node dist/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "prepare": "husky install"
  },
  "keywords": [
    "apollo-server-express",
    "graphql",
    "typescript"
  ],
  "author": "Bluetorch",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.5",
    "@graphql-codegen/typescript-resolvers": "2.5.2",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.17.11",
    "@types/node": "16.11.25",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "concurrently": "7.0.0",
    "eslint": "8.9.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "^7.0.4",
    "jest": "27.5.1",
    "jest-html-reporter": "3.4.2",
    "jest-sonar-reporter": "2.0.0",
    "jsdoc": "3.6.10",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "typescript": "4.5.5",
    "wait-on": "6.0.1"
  },
  "dependencies": {
    "@envelop/graphql-jit": "^5.0.4",
    "@fastify/compress": "^6.2.0",
    "@fastify/cors": "^8.2.0",
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/merge": "^8.2.2",
    "@graphql-tools/mock": "^8.7.14",
    "@graphql-tools/schema": "^8.3.1",
    "@graphql-tools/utils": "^8.6.1",
    "@graphql-tools/wrap": "^8.4.0",
    "@graphql-yoga/render-graphiql": "^3.1.2",
    "axios": "^0.26.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-undici-fetch": "^0.1.24",
    "fastify": "^4.10.2",
    "graphql": "^16.0.0",
    "graphql-yoga": "^3.2.1",
    "lodash": "^4.17.20",
    "winston": "^3.3.3",
    "winston3-logstash-udp": "0.0.7"
  }
}
